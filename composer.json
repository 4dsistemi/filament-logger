{
    "name": "4dsistemi/filament-logger",
    "description": "Activity logger for filament",
    "keywords": [
        "4DSISTEMI",
        "laravel",
        "filament-logger"
    ],
    "homepage": "https://github.com/4dsistemi/filament-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Edoardo Gullo",
            "email": "edoardo.gullo@4dsistemi.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^8.0 | ^9.0 | ^10.0 | ^11.0 | ^12.0",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-package-tools": "^1.13.5"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Qdsistemi\\FilamentLogger\\": "src",
            "Qdsistemi\\FilamentLogger\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qdsistemi\\FilamentLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qdsistemi\\FilamentLogger\\FilamentLoggerServiceProvider"
            ],
            "aliases": {
                "FilamentLogger": "Qdsistemi\\FilamentLogger\\Facades\\FilamentLogger"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}